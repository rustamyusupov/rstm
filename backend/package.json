    "express": "^4.17.2",
    "json-server": "^0.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "nodemon": "^2.0.15",
    "parcel": "^2.1.1"
  }
}
