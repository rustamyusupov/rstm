{
  "name": "rstm",
  "version": "1.0.0",
  "description": "Personal stuff about me",
  "license": "MIT",
  "private": true,
  "author": "Rustam Yuspov",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "NODE_ENV=production babel src --out-dir dist/server --source-maps inline && cp -r public dist/public && cp -r src/views dist/server/views",
    "start": "NODE_ENV=production node dist/server/bin/server.js",
    "dev": "nodemon --exec babel-node src/bin/server.js",
    "lint": "eslint ."
  },
  "dependencies": {
    "express": "^4.17.2",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "eslint": "^8.6.0",
    "nodemon": "^2.0.15"
  }
}
